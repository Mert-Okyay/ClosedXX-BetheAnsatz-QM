 RowBox[{
  RowBox[{"measureS2", "[", "ket_", "]"}], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "L", ",", "s2", ",", "ssplus1", ",", "A", ",", "sols", ",", " ", 
