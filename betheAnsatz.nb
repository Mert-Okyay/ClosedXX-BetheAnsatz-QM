Cell[BoxData[
 RowBox[{
  RowBox[{"measureS2", "[", "ket_", "]"}], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "L", ",", "s2", ",", "ssplus1", ",", "A", ",", "sols", ",", " ", 
      "ssquared", ",", "sol"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"L", " ", "=", " ", 
      RowBox[{"Log", "[", 
       RowBox[{"2", ",", " ", 
        RowBox[{"Length", "@", "ket"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"sol", " ", "=", " ", 
      RowBox[{"Flatten", "@", 
       RowBox[{"Quiet", "@", 
        RowBox[{"Solve", "[", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{"totalS", "[", "L", "]"}], "-", 
              RowBox[{"A", "*", 
               RowBox[{"IdentityMatrix", "[", 
                SuperscriptBox["2", "L"], "]"}]}]}], ")"}], ".", "ket"}], "==",
            "0"}], ",", "A"}], "]"}]}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"ssplus1", " ", "=", " ", 
      RowBox[{"Quiet", "@", 
       RowBox[{"(", 
        RowBox[{"A", "/.", 
         RowBox[{"sol", "[", 
          RowBox[{"[", "1", "]"}], "]"}]}], ")"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Length", "@", "sol"}], "<", "1"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
       "Print", "[", 
        "\"\<Not an eigenstate of \!\(\*SuperscriptBox[\(S\), \(2\)]\).\>\"", 
        "]"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"ssquared", " ", "=", " ", 
         RowBox[{"Max", "@", 
          RowBox[{"(", 
           RowBox[{"s2", "/.", 
            RowBox[{"Solve", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"s2", 
